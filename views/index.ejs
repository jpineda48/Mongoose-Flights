<%- include('./partials/header') %>

<h3>ALL FLIGHTS</h3>
<table>
    <thead>
        <tr>
            
            <th>Airline</th>
            <th>Airport</th>
            <th></th>
        
        </tr>
    </thead>
    <tbody>
        <% flights.forEach(function(f) { %>
        <tr>
            
            
            <td><%= f.airline %> |</td>
            <td><%= f.airport %> |</td>
            <td><a href="/flights/<%= f.id%>">DETAILS</a></td>
        </tr>


        <% })%>
    </tbody>
    
</table>





<%- include('./partials/footer') %>
